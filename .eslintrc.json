{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": ["/dist/*"],
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-undef": 0
  }
}
